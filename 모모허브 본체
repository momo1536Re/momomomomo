    --- light
    
    MainSection:NewToggle("Spam Skill(Light)", "This is Spam Skill", function(Light) -- 완료
        if Light then
            script = game:GetService("Players").LocalPlayer.Character.Powers.Light;
            momoscript = script.RemoteEvent.RemoteFunction:InvokeServer();
            local pla = game.Players.LocalPlayer;
            local Mouse = pla:GetMouse();
            _G.light = true
            while _G.light do
                local args = {
                    [1] = momoscript,
                    [2] = "LightPower2",
                    [3] = "StopCharging",
                    [4] = Mouse.Hit,
                    [5] = pla,
                    [6] = 100
                }
                game:GetService("Players").LocalPlayer.Character.Powers.Light.RemoteEvent:FireServer(unpack(args))
                wait(0.1)
            end
        else
            _G.light = false
        end
    end)
    
