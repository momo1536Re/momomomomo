-------------------------------------Misc

local Misc = Window:NewTab("Misc")

--superjump
local MiscSection = Misc:NewSection("JumpPower")

MiscSection:NewSlider("JumpPower", "JumpPower up", 1000, 50, function(s) -- 500 (MaxValue) | 0 (MinValue)
	game:GetService("Players").LocalPlayer.Character.Humanoid.JumpPower = (s)
end)


-- get userid
local id = game.Players.LocalPlayer.userId
print(id)

--haki
local MiscSection = Misc:NewSection("Haki Farm")

_G.haki = true
MiscSection:NewToggle("buso haki(test-no working)", "good", function(s)
    if s then
        while _G.haki do
            workspace.UserData.User_338675324.UpdateHaki:FireServer()
            wait(0.1)
        end
    else
		_G.haki = false
    end
end)

-----------------------------buy juice
local buy = Window:NewTab("buy")
local buySection = buy:NewSection("Buy water+(need money)")

local TXTSecond;

-- text box(seconds)

buySection:NewTextBox("Water+ Seconds", "The number of water+", function(txt)
	TXTSecond = txt;
	print(txt)
end)


-- buy cider+

buySection:NewButton("buy Cider+", "the number of juice", function()
	getgenv().buywater = true;
    function buywater(buyType, buyLimit)
        spawn(function()
            local iteration = TXTSecond;
            while wait() do
                if not buywater or iteration == buyLimit then break end;
                local args = {[1] = "Cider+"}
                workspace.Merchants.BetterDrinkMerchant.Clickable.Retum:FireServer(unpack(args))
                iteration = iteration -1
            end
        end)
    end
    buywater('Cider+', 0)
end)

-- buy Lemonade+

buySection:NewButton("buy Lemonade+", "the number of juice", function()
	getgenv().buywater = true;
    function buywater(buyType, buyLimit)
        spawn(function()
            local iteration = TXTSecond;
            while wait() do
                if not buywater or iteration == buyLimit then break end;
                local args = {[1] = "Lemonade+"}
                workspace.Merchants.BetterDrinkMerchant.Clickable.Retum:FireServer(unpack(args))
                iteration = iteration -1
            end
        end)
    end
    buywater('Lemonade+', 0)
end)

