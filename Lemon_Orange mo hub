------------------------------Test
local Farm = Window:NewTab("Farm")
local FarmSection = Farm:NewSection("test")
-- magma farm

local SelectedSecond;

FarmSection:NewDropdown("Seconds", "set Second", {"1", "5", "10"}, function(value)
    SelectedSecond = value;
	print(value)
end)

FarmSection:NewButton("ClickRod", "Click Super Rod", function()
	getgenv().Rod = true;
	function clickRod(buyType, buyLimit)
		spawn(function()
			local iteration = SelectedSecond;
			while wait(1) do
				if not Rod or iteration == buyLimit then break end;
				local args = {[1] = Vector3.new(-226.27146911621094, 213, -443.7113952636719)}
				game:GetService("Players").LocalPlayer.Character:FindFirstChild("Super Rod").Click:FireServer(unpack(args))
				iteration = iteration -1
			end
		end)
	end
	clickRod("Rod", 0)
end)
