local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Mo hub - 0.1.0", "Ocean")

-------------------------------------------main

local Main = Window:NewTab("Main")

--- spam skills

local MainSection = Main:NewSection("Spam skill(Please use the skill once and use it)")

MainSection:NewLabel("Reset if you want to turn off Spam skill")

--- light

MainSection:NewToggle("Spam Skill(Light)", "This is Spam Skill", function(Light) -- 완료
    if Light then
        script = game:GetService("Players").LocalPlayer.Character.Powers.Light;
        sexmo_mo1536Re = script.RemoteEvent.RemoteFunction:InvokeServer();
        local pla = game.Players.LocalPlayer;
        local Mouse = pla:GetMouse();
        _G.light = true
        while _G.light do
            local args = {
                [1] = sexmo_mo1536Re,
                [2] = "LightPower2",
                [3] = "StopCharging",
                [4] = Mouse.Hit,
                [5] = pla,
                [6] = 100
            }
            game:GetService("Players").LocalPlayer.Character.Powers.Light.RemoteEvent:FireServer(unpack(args))
            wait(0.5)
        end
    else
        _G.light = false
    end
end)

--- magma

MainSection:NewToggle("Spam Skill(Magma)", "This is Spam Skill", function(Magma)
