local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Mo hub", "Ocean")


local id = game.Players.LocalPlayer.userId
print(id)


-------------------------------------------main

local Main = Window:NewTab("Main")

--- spam skills

local MainSection = Main:NewSection("Spam skill(Please use the skill once and use it)")

MainSection:NewLabel("Reset if you want to turn off Spam skill")

MainSection:NewButton("Spam Skill(Light)", "This is Spam Skill", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/momo1536Re/momomomomo/main/light%20spam'))()
end)

MainSection:NewButton("Spam Skill(Magma)", "This is Spam Skill", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/momo1536Re/momomomomo/main/magma%20spam'))()
end)

MainSection:NewButton("Spam Skill(Dark)", "This is Spam Skill", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/momo1536Re/momomomomo/main/dark%20spam'))()
end)

MainSection:NewButton("Spam Skill(Quake)", "This is Spam Skill", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/momo1536Re/momomomomo/main/Quake%20spam'))()
end)

MainSection:NewButton("Spam Skill(Bomb)", "This is Spam Skill", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/momo1536Re/momomomomo/main/bomb%20spam%20skill"))()
end)

-------------------------------------Misc

local Misc = Window:NewTab("Misc")

--superjump
local MiscSection = Misc:NewSection("JumpPower")

MiscSection:NewSlider("JumpPower", "JumpPower up", 1000, 50, function(s) -- 500 (MaxValue) | 0 (MinValue)
	game:GetService("Players").LocalPlayer.Character.Humanoid.JumpPower = (s)
end)

--haki
local MiscSection = Misc:NewSection("Haki Farm")

MiscSection:NewToggle("buso haki(test-no working)", "good", function(s)
    if s then
		_G.haki = true
        while _G.haki == true do
            workspace.UserData.User_1433174631.UpdateHaki:FireServer()
            wait(0.1)
        end
    else
		_G.haki = false
        while wait(0.1) do
            if not _G.haki then
                break
            end;
			workspace.UserData.User_1433174631.UpdateHaki:FireServer()
        end
    end
end)

-----------------------------buy juice
local buy = Window:NewTab("buy")

-- buy cider+
local buySection = buy:NewSection("Buy water+(need money)")

buySection:NewButton("buy Cider+", "the number of juice", function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/momo1536Re/momomomomo/main/Cider%2B"))()
end)

-- buy Lemonade+

buySection:NewButton("buy Lemonade+", "the number of juice", function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/momo1536Re/momomomomo/main/Lemonade%2B"))()
end)

--buy Juice+

